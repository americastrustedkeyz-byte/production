<odoo>
    <data>

        <record id="atk_launch_cron" model="ir.cron">
            <field name="name">ATK Launch Switch</field>
            <field name="model_id" ref="website.model_website_page"/>
            <field name="state">code</field>
            <field name="code">
homepage = env.ref('atk_website.atk_homepage')
countdown = env.ref('atk_website.atk_countdown_page')
homepage.website_published = True
countdown.website_published = False
            </field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">1</field>
            <field name="nextcall">2026-01-19 00:01:00</field>
        </record>

    </data>
</odoo>
