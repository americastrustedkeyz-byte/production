<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <!-- Homepage View -->
        <record id="view_atk_homepage" model="ir.ui.view">
            <field name="name">atk.homepage.view</field>
            <field name="type">qweb</field>
            <field name="arch" type="xml">
                <![CDATA[
                <t t-call="theme_atk_navy.hero"/>
                ]]>
            </field>
        </record>

        <!-- Homepage Page -->
        <record id="page_atk_homepage" model="website.page">
            <field name="name">ATK Home</field>
            <field name="url">/</field>
            <field name="view_id" ref="view_atk_homepage"/>
            <field name="is_published">False</field>
        </record>

    </data>
</odoo>
